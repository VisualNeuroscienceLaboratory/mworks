<?xml version="1.0"?>
<monkeyml version="1.0">
    <filters>
        <filter tag="Boxcar_eyeH" full_name="Boxcar_eyeH" id="" type="boxcar_filter_1D" in1="eye_h_calibrated" out1="eye_h" width_samples="5"/>
        <filter tag="Boxcar_eyeV" full_name="Boxcar_eyeV" id="" type="boxcar_filter_1D" in1="eye_v_calibrated" out1="eye_v" width_samples="5"/>
        <filter tag="eyeMonitor" full_name="eyeMonitor" id="" type="basic_eye_monitor" eyeh_calibrated="eye_h" eyev_calibrated="eye_v" eye_state="saccade" width_samples="5" saccade_entry_speed="60" saccade_exit_speed="10"/>
    </filters>
    <io_devices>
        <iodevice alt="FM" tag="Setup 1 ITC18" priority="" type="ITC18" id="">
            <iochannel variable="eye_h_raw" capability="ITC18_ADC_INPUT0" data_type="analog" direction="input" synchrony="hardware_timed_synchronous" range_min="-10" range_max="10" resolution="16" update_interval="3ms" data_interval="1ms" tag="~ELEMENTNAME~~ELEMENTCOUNT~"/>
            <iochannel variable="eye_v_raw" capability="ITC18_ADC_INPUT1" data_type="analog" direction="input" synchrony="hardware_timed_synchronous" range_min="-10" range_max="10" resolution="16" update_interval="3ms" data_interval="1ms" tag="~ELEMENTNAME~~ELEMENTCOUNT~"/>
        </iodevice>
		<iodevice tag="FM" priority="" type="fake_monkey" id="">
            <iochannel type="eye_movement" eye_v="eye_v_raw" eye_h="eye_h_raw" data_interval="1ms" update_interval="10ms"/>
        </iodevice>
    </io_devices>
    <calibrators>
        <calibrator tag="eyeCalibrator" full_name="eyeCalibrator" type="standard_eye_calibrator" eyeh_raw="eye_h_raw" eyev_raw="eye_v_raw" eyeh_calibrated="eye_h_calibrated" eyev_calibrated="eye_v_calibrated"/>
    </calibrators>
    <variables>
        <variable tag="eye_h_raw_log" full_name="eye_h_raw_log" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed"/>
        <variable tag="eye_h_log" full_name="eye_h_raw" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed"/>
        <variable tag="eye_h_calibrated_log" full_name="eye_h_raw" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed"/>
        <variable tag="saccade_log" full_name="saccade_register" default_value="0" scope="GLOBAL" type="INTEGER" editable="never" logging="when_changed"/>
        <variable tag="saccade" full_name="saccade" default_value="0" scope="GLOBAL" type="INTEGER" editable="never" logging="when_changed">
            <action tag="Assignment_1" full_name="fullname_1" type="assignment" variable="saccade_log" value="1"/>
        </variable>
        <variable tag="eye_h_raw" full_name="eye_h_raw" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed">
            <action tag="Assignment_4" full_name="fullname_4" type="assignment" variable="eye_h_raw_log" value="1"/>
        </variable>
        <variable tag="eye_v_raw" full_name="eye_v_raw" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed"/>
        <variable tag="eye_h_calibrated" full_name="eye_h_calibrated" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed">
            <action tag="Assignment_5" full_name="fullname_5" type="assignment" variable="eye_h_calibrated_log" value="1"/>
        </variable>
        <variable tag="eye_v_calibrated" full_name="eye_v_calibrated" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed"/>
        <variable tag="eye_h" full_name="eye_h" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed">
            <action tag="Assignment_6" full_name="fullname_6" type="assignment" variable="eye_h_log" value="1"/>
        </variable>
        <variable tag="eye_v" full_name="eye_v" default_value="0" scope="GLOBAL" type="FLOAT" editable="never" logging="when_changed"/>
    </variables>
    <stimuli/>
    <sounds/>
    <experiment tag="It is your boy" full_name="Super Awesome Happy">
        <protocol tag="refprot" full_name="sup" interruptable="YES" selection="sequential_ascending" nsamples="1" sampling_method="cycles" description="" interruptible="YES">
            <block tag="sup2" full_name="supsup" interruptable="YES" selection="sequential_ascending" nsamples="1" sampling_method="cycles" description="" interruptible="YES">
                <task_system tag="task_system12" full_name="fullname12" interruptable="YES" description="" interruptible="YES">
                    <task_system_state tag="task_system_state13" full_name="fullname13" interruptable="YES" description="" interruptible="YES">
                        <action tag="action14" full_name="fullname14" type="start_device_IO" device="Setup 1 ITC18"/>
                        <action tag="action15" full_name="fullname15" type="start_timer" timer="timer" duration="15000ms"/>
                        <transition tag="transition18" full_name="fullname18" target="task_system_state16" type="timer_expired" timer="timer"/>
                    </task_system_state>
                    <task_system_state tag="task_system_state16" full_name="fullname16" interruptable="YES" description="" interruptible="YES">
                        <action tag="action19" full_name="fullname19" type="stop_device_IO" device="Setup 1 ITC18"/>
                        <transition type="yield" tag="transition17" full_name="fullname17"/>
                    </task_system_state>
                </task_system>
            </block>
        </protocol>
    </experiment>
</monkeyml>