<?xml version="1.0"?>
<monkeyml version="1.0">
    <io_devices>
        <iodevice tag="sITC" priority="" type="ITC18" id="">
            <iochannel variable="eye_h" capability="ITC18_ADC_INPUT0" data_type="analog" direction="input" synchrony="hardware_timed_synchronous" range_min="-10" range_max="10" resolution="16" update_interval="3ms" data_interval="1ms" tag="Eye_H_channel" TTL_trigger_port="0" pre_trigger_interval="5ms" post_trigger_interval="5ms"/>
