<?xml version="1.0"?>
<monkeyml version="1.0">
    <variables>
        <variable tag="success" full_name="some Var" default_value="0" scope="GLOBAL" type="DOUBLE" editable="ALWAYS" logging="WHEN_CHANGED"/>
        <variable tag="failure" full_name="some Var" default_value="0" scope="GLOBAL" type="DOUBLE" editable="ALWAYS" logging="WHEN_CHANGED"/>
        <variable tag="ignored" full_name="some Var" default_value="0" scope="GLOBAL" type="DOUBLE" editable="ALWAYS" logging="WHEN_CHANGED"/>
        <variable tag="some_Var" full_name="some Var" default_value="0" scope="GLOBAL" type="DOUBLE" editable="ALWAYS" logging="WHEN_CHANGED"/>
        <variable tag="some_Var2" full_name="some Var" default_value="0" scope="GLOBAL" type="DOUBLE" editable="ALWAYS" logging="WHEN_CHANGED"/>
        <variable tag="theTimer" full_name="some Var" default_value="0" scope="GLOBAL" type="DOUBLE" editable="ALWAYS" logging="WHEN_CHANGED"/>
    </variables>
    <stimuli>
        <stimulus tag="blank screen" full_name="bland screen" type="blank_screen" color="1,0,0"/>
    </stimuli>
    <experiment tag="theExperiment" full_name="the Experiment">
        <protocol tag="theProtocol" full_name="the Protocol" interruptable="YES" selection="sequential_ascending" nsamples="1" sampling_method="cycles" description="" interruptible="YES">
            <block tag="theBlock" full_name="the Block" interruptable="YES" selection="sequential_ascending" nsamples="1" sampling_method="cycles" description="" interruptible="YES">
                <trial tag="theTrial" full_name="the Trial" description="" interruptible="YES" selection="sequential_ascending" nsamples="1" sampling_method="cycles">
                    <task_system tag="theTaskSystem">
                        <task_system_state tag="theState" full_name="the State" description="" interruptible="YES">
                            <action tag="some_Var assignment" full_name="fullname_1" type="assignment" variable="some_Var" value="6"/>
                            <action tag="some_Var assignment" full_name="fullname_1" type="assignment" variable="some_Var2" value="126"/>
                            <transition type="yield"/>
                        </task_system_state>
                    </task_system>
                </trial>
            </block>
        </protocol>
    </experiment>
</monkeyml>