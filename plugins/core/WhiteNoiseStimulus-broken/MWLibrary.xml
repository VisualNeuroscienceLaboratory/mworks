<MWElements>
	<MWElement name="White Noise Stimulus">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>stimulus[@type="white_noise]</match_signature>
		
		<isa>Stimulus</isa>
		
		<description>
			A white noise stimulus -
        </description>
		
		<!-- Payload -->
		<code>
			<stimulus type="white_noise" tag="New White Noise Texture"
            distribution_type="ternary"
            mask="rectangle"
            scrx="0"
            scry="0"
            rotation="0"
            opacity="1"
            check_size_x="1"
            check_size_y="1"
            n_checks_x="1"
            n_checks_y="1"
            dist_mu="0"
            dist_rng="1"
            autoplay="0"/>
        </code>
    </MWElement>
</MWElements>

