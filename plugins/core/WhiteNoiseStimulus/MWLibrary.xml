<MWElements>
	<MWElement name="White Noise Stimulus">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>stimulus[@type="white_noise"]</match_signature>
		
		<isa>Stimulus</isa>
		
		<description>
			A white noise stimulus
        </description>
		
		<!-- Payload -->
		<code>
			<stimulus type="white_noise" tag="New White Noise"
            noise="ternary"
            mask="rectangle"
            x_position="0"
            y_position="0"
            x_size="1"
            y_size="1"
            rotation="0"
            opacity="1"
            n_cols="51"
            n_rows="51"
            distrib_mu="0"
            distrib_range="1"
            nframes="3"
            seed="0"
            autoplay="0"/>
        </code>
    </MWElement>
</MWElements>

